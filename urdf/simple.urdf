<robot name="myrobot">

<link name="base_link">
<visual>
	<origin xyz="0 0 0.80" rpy="0 0 0"/>
		<geometry>
		<box size="1 1 1.2"/>
	</geometry>
</visual>	
</link>

<link name="part1">
<visual>
  <origin xyz="0 0 0.425" rpy="0 0 0"/>
    <geometry>
       <box size="0.5 0.5 0.85"/>
    </geometry>
</visual>
</link>
<link name="part2">
<visual>
  <origin xyz="0 0 0.7875" rpy="0 0 0"/>
    <geometry>
      <cylinder length="1.575" radius="0.20"/>
    </geometry>
</visual>
</link>
<link name="part3">
<visual>
  <origin xyz="0 0 0.9167" rpy="0 0 0"/>
    <geometry>
      <cylinder length="1.8334" radius="0.20"/>
    </geometry>
</visual>
</link>
<link name="part4">
<visual>
  <origin xyz="0 0 0.7308" rpy="0 0 0"/>
    <geometry>
      <cylinder length="1.4616" radius="0.20"/>
    </geometry>
</visual>
</link>



<joint name="joint0" type="continuous">
    <parent link="base_link"/>
    <child link="part1"/>
    <origin xyz="0 0 1.2" rpy="0 0 0" /> 
    <axis xyz="0 0 1" rpy="0 0 0" />
  </joint>

<joint name="joint1" type="continuous">
    <parent link="part1"/>
    <child link="part2"/>
    <origin xyz="0 0 0.85" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>
<joint name="joint2" type="continuous">
    <parent link="part2"/>
    <child link="part3"/>
    <origin xyz="0 0 1.575" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>
<joint name="joint3" type="continuous">
    <parent link="part3"/>
    <child link="part4"/>
    <origin xyz="0 0 1.8334" rpy="0 0 0" /> 
    <axis xyz="0 1 0" rpy="0 0 0" />
  </joint>
</robot>
